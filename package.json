{
  "name": "minista-project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "minista",
    "build": "minista build",
    "preview": "minista preview",
    "lint:js": "eslint src --ext .js,.jsx",
    "lint:js:fix": "eslint src --ext .js,.jsx --fix",
    "lint:css": "stylelint \"scr/**/*.{css,scss,pcss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss,pcss}\" --fix",
    "prettier": "prettier --check src/**/*",
    "prettier:fix": "prettier --write src/**/*",
    "lint": "npm-run-all --parallel lint:js lint:css prettier",
    "lint:fix": "npm-run-all lint:js:fix lint:css:fix prettier:fix",
    "predeploy": "minista build && echo.> dist/.nojekyll",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "quoteProps": "consistent",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "endOfLine": "lf",
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "embeddedLanguageFormatting": "auto",
    "insertPragma": false,
    "requirePragma": false
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.0",
    "gh-pages": "^6.3.0",
    "@babel/plugin-syntax-jsx": "7.27.1",
    "@eslint/js": "9.30.1",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.3.0",
    "minista": "3.1.12",
    "npm-run-all": "4.1.5",
    "postcss-preset-env": "10.2.4",
    "postcss-pxtorem": "6.1.0",
    "prettier": "3.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.89.2",
    "stylelint": "16.21.0",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-order": "7.0.0",
    "stylelint-scss": "6.12.1",
    "stylelint-selector-bem-pattern": "4.0.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "dependencies": {
    "@a1rth/css-normalize": "^1.2.1",
    "clsx": "^2.1.1",
    "imask": "^7.6.1"
  },
  "overrides": {
    "typescript-eslint": {
      "eslint": "9.30.1"
    },
    "@typescript-eslint/parser": {
      "eslint": "9.30.1"
    },
    "@typescript-eslint/eslint-plugin": {
      "eslint": "9.30.1"
    }
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead"
  ]
}
